{
  "name": "kukuri-client",
  "version": "1.0.2b3",
  "description": "A selfbot that wont betray you. [Copyright Â© Mikasuru 2024]",
  "main": "Main.js",
  "author": {
    "name": "Mikasuru",
    "email": "kukuri.dev@gmail.com",
    "url": "https://www.youtube.com/@kukuri_xyz"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mikasuru/KukuriClient/issues"
  },
  "github": "https://github.com/Mikasuru/KukuriClient",
  "scripts": {
    "cli": "bun run Main.js",
    "web": "bun run Server/app.js",
    "start": "electron Application/main.cjs",
    "build": "electron-builder"
  },
  "dependencies": {
    "@discordjs/opus": "0.9.0",
    "@distube/ytdl-core": "4.15.8",
    "@stablelib/xchacha20poly1305": "2.0.0",
    "axios": "1.7.7",
    "canvas": "3.0.0",
    "discord.js-selfbot-v13": "3.4.6",
    "express": "4.21.1",
    "ffmpeg": "0.0.4",
    "libsodium-wrappers": "0.7.15",
    "node-notifier": "10.0.1",
    "opusscript": "0.1.1",
    "sharp": "0.33.5",
    "sodium": "3.0.2",
    "systeminformation": "5.23.25",
    "universal-speedtest": "3.0.0",
    "ytdl-core": "4.11.5"
  },
  "devDependencies": {
    "electron": "^33.2.1",
    "electron-builder": "^24.9.1",
    "electron-store": "^8.1.0"
  },
  "build": {
    "appId": "com.kukuri.client",
    "productName": "Kukuri Client",
    "directories": {
      "output": "dist"
    },
    "files": [
      "Application/**/*",
      "Server/**/*",
      "Module/**/*",
      "Config/**/*",
      "Commands/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "uninstallDisplayName": "Kukuri Client",
      "createStartMenuShortcut": true
    }
  }
}
